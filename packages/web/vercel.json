    { "source": "/sitemap.xml", "destination": "/sitemap.xml" },
    { "source": "/googlece427fe68cd7c48b.html", "destination": "/googlece427fe68cd7c48b.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
